{
  "name": "smarthome-api",
  "version": "2.2.7",
  "main": "build/server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/server.js",
    "build": "npm run lint && tsc",
    "dev": "npm run build && NODE_ENV=development node build/server.js",
    "dev:watch": "npm run build -- --watch & NODE_ENV=development nodemon build/server.js",
    "docker:generate": "docker build -t $(node -pe \"require('./package.json').name.concat(':').concat(require('./package.json').version)\") .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "tag": "TAG=v$(node -pe \"require('./package.json').version\"); git commit -am \"ðŸ”– $TAG\" && git tag $TAG && git push && git push --tags"
  },
  "author": "Ulises Santana <ulisesantana@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "8.2.0",
    "fastify": "3.9.2",
    "fastify-autoload": "3.4.0",
    "fastify-cors": "5.2.0",
    "fastify-plugin": "3.0.0",
    "fastify-static": "3.3.0",
    "fastify-swagger": "3.5.0",
    "fluent-json-schema": "2.0.3",
    "lit-element": "2.4.0",
    "request": "2.88.2",
    "tplink-smarthome-api": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "14.14.16",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.16.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "nodemon": "2.0.6",
    "pino-pretty": "4.3.0",
    "ts-essentials": "7.0.1",
    "typescript": "3.9.7"
  },
